.all{
  width: 100%;
  height: 1000rpx;
  background: #efefef;
}
.cent{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.cent image:nth-child(1){
  margin-top: 200rpx;
  width: 200rpx;
  height:200rpx;
}
.cent text:nth-child(2){
  font-size: 40rpx;
  color:#333333;
  font-weight: bolder;
}
.cent text:nth-child(3){
  font-size: 30rpx;
  color:#939393; 
}
button{
  color:#1e1e1e;
  font-size: 26rpx;
}